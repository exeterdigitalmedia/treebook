<div class="row">
<div class="span4">
<%= simple_form_for(resource, as: resource_name, url: registration_path(resource_name), html: {class: "well"}) do |f| %>
  <%= devise_error_messages! %>
  <fieldset>
    <legend>Sign up</legend>
  <%= f.input :first_name %> <%= f.input :last_name %>
  <%= f.input :profile_name %> <%= f.input :email %>
  <%= f.input :password %> <%= f.input :password_confirmation, autocomplete: "off" %>
  <div class="actions">
    <%= f.button :submit, "Sign up" %>
  </div>
<% end %>
</fieldset>
</div>
<div class="row">
<div class="span8">
  <div class="well">
    <h3>Its easy to register, safe and secure, alternitively you can Log in below</h3>
<%= render "devise/shared/links" %>
</div>
</div>